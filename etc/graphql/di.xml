<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:ObjectManager/etc/config.xsd">
    <type name="Magento\StoreGraphQl\Model\Resolver\Store\StoreConfigDataProvider">
        <arguments>
            <argument name="extendedConfigData" xsi:type="array">
                <item name="city_region_postcode_module_enabled"
                      xsi:type="string">mageinic_cityregionpostcode/general/enabled</item>
                <item name="country_searchable" xsi:type="string">mageinic_cityregionpostcode/country/searchable</item>
                <item name="region_searchable" xsi:type="string">mageinic_cityregionpostcode/region/searchable</item>
                <item name="city_enable" xsi:type="string">mageinic_cityregionpostcode/city/enable</item>
                <item name="city_searchable" xsi:type="string">mageinic_cityregionpostcode/city/searchable</item>
                <item name="postcode_enable" xsi:type="string">mageinic_cityregionpostcode/postcode/enable</item>
                <item name="postcode_searchable"
                      xsi:type="string">mageinic_cityregionpostcode/postcode/searchable</item>
            </argument>
        </arguments>
    </type>
</config>
